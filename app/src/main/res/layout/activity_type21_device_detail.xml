<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/constraintMain"
    android:layout_height="match_parent"
    android:background="@color/backgroundclr"
    tools:context=".main.home.devicedetails.type21.Type21DeviceDetailActivity">

    <include layout="@layout/home_header"></include>


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvGroupsSelected"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/homehead"
        tools:listitem="@layout/group_selection_item"></androidx.recyclerview.widget.RecyclerView>

    <include layout="@layout/layout_progress_bar"></include>

</androidx.constraintlayout.widget.ConstraintLayout>